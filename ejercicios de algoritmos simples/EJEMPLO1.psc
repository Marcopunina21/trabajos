Algoritmo EJEMPLO1
	
	Definir NUM1,NUM2,SUM,RES,MUL,DIV Como Real
	Escribir 'INGRESE NUM 1'
	Leer NUM1
	Escribir 'INGRESE NUM2'
	Leer NUM2
	SUM <- NUM1+NUM2
	RES <- NUM1-NUM2
	MUL <- NUM1*NUM2
	DIV <- NUM1/NUM2
	Escribir 'LA SUM DE: ',NUM1,' + ',NUM2,'ES:',SUM
	Escribir 'LA RES DE: ',NUM1,' + ',NUM2,'ES:',RES
	Escribir 'LA MUL DE: ',NUM1,' + ',NUM2,'ES:',MUL
	Escribir 'LA DIV DE: ',NUM1,' + ',NUM2,'ES:',DIV
FinAlgoritmo
